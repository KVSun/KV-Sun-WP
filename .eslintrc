{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "sourceType": "module"
    },
    "rules": {
        "indent": [
            1,
            "tab"
        ],
        "quotes": [
            1,
            "single"
        ],
        "semi": [
            2,
            "always"
        ],
        "no-console": 0,
        "no-mixed-spaces-and-tabs": 1,
        "no-unused-vars": 1,
        "no-undef": 0,
        "no-redeclare": 1,
        "no-empty": 1,
        "no-cond-assign": 0,
        "no-sparse-arrays": 0,
        "no-self-assign": 0,
        "no-inner-declarations": 0,
        "no-extra-boolean-cast": 0
    },
    "globals": {
        "URLSearchParams": false,
        "jQuery": false,
        "ga": false,
        "$": false,
        "define": false
    }
}
